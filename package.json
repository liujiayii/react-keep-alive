{
  "name": "react-keepalive",
  "type": "module",
  "version": "0.0.0",
  "description": "A keep-alive component for React",
  "authors": "liujiayii",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "eslint .",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint-react/eslint-plugin": "^2.3.1",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@unocss/eslint-plugin": "^66.5.4",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.0.2",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.10",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsdown": "^0.16.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "^19.2.2",
      "@types/react-dom": "^19.2.2"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "pnpm exec eslint --cache --fix"
    ]
  }
}
